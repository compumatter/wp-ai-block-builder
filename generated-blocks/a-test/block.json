{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "cm/a-test",
  "title": "A Test",
  "category": "custom-blocks",
  "icon": "admin-customizer",
  "description": "A simple block that displays Hello Test in large text",
  "keywords": [
    "test",
    "hello"
  ],
  "version": "1.0.0",
  "textdomain": "cm",
  "attributes": {
    "message": {
      "type": "string"
    },
    "fontSize": {
      "type": "string"
    },
    "className": {
      "type": "string"
    }
  },
  "supports": {
    "html": false
  },
  "editorScript": "a-test-editor-js",
  "script": "a-test-centralized-js",
  "style": "a-test-style"
}